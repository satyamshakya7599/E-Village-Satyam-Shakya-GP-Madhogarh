@{
    ViewBag.Title = "SignupMGMT";
    Layout = "~/Views/Shared/admin master.cshtml";
}


@model List<mvcdemo2.Models.registration>
<div class="container">
    <div class="row">
        <div class="col-md-12">
        <table class="table table-bordered table-hover"
    style="background:linear-gradient(#A5D6A7,#2E7D32)">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Mobile</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Profile</th>
                <th>Qualification</th>
                <th>Address</th>
                <th>District</th>
                <th>DateOfBirth</th>
                <th>Registration Date</th>
                <th>Password</th>
                <th>Add</th>
                <th>Delete</th>
                <th>Update</th>
            </tr>
            @foreach (var x in Model)
            { 
            <tr>
                <td>@x.id</td>
                <td>@x.name</td>
                <td>@x.mob</td>
                <td>@x.gender</td>
                <td>@x.email</td>
                <td><img src="~/Content/img/@x.profile"height="50"width="50"/></td>
                <td>@x.qualification</td>
                <td>@x.address</td>
                <td>@x.district</td>
                <td>@x.dob</td>
                <td>@x.regdate</td>
                <td>@x.pass</td>
                <td><a href="#"><span class="fa fa-plus" style="font-size:30px;"></span></a></td>
                <td><a href="/Admin/DeleteSi/@x.id"><span class="fa fa-trash" style="font-size:30px;color:red;"> </span></a></td>
                <td><a href="/Admin/Updatereg?U=@x.email"><span class="fa fa-edit">edit</span></a></td>
            </tr>
            }
        </table>
            </div>
    </div>
</div>

